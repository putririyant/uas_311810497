<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.ac.pelitabangsa.ericofernandy_uas">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EricoFernandy_UAS">
        <activity android:name=".Activity.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.HomeScreen"></activity>
        <activity android:name=".Activity.SignActivity"></activity>
        <activity android:name=".Activity.RegisterActvity"></activity>
        <activity android:name=".Activity.Home"></activity>
        <activity android:name=".Activity.MainActivity"></activity>
        <activity android:name=".Activity.ViewData"></activity>
        <activity android:name=".Activity.UpdateActivity"></activity>

    </application>

</manifest>